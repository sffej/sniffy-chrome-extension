{
  "manifest_version": 2,
  "name": "Sniffy Chrome Extension",
  "description": "Sniffy Chrome Extension adds Enable-Sniffy headers to all requests, thus allowing you to enable Sniffy on demand",
  "version": "0.0.1",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "off.png",
    "default-title": "Sniffy"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "on.png",
    "off.png"
  ]
}